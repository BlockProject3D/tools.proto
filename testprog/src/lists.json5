{
    name: "lists",
    imports: [
        { protocol: "unions", type: "Item" }
    ],
    types: [
        {
            name: "VarList",
            info: {
                type: "list",
                max_len: 255,
                item_type: "Item",
                nested: true
            }
        },
        {
            name: "RunList",
            info: {
                type: "list",
                max_len: 999999,
                item_type: "SpanRun"
            }
        }
    ],
    structs: [
        {
            name: "Times",
            fields: [
                {
                    name: "start",
                    info: {
                        type: "unsigned",
                        bits: 64
                    }
                },
                {
                    name: "end",
                    info: {
                        type: "unsigned",
                        bits: 64
                    }
                }
            ]
        }
    ],
    messages: [
        {
            name: "SpanRun",
            fields: [
                { name: "times", item_type: "Times" },
                { name: "vars", item_type: "VarList" }
            ]
        },
        {
            name: "Dataset",
            fields: [
                { name: "runs", item_type: "RunList" }
            ]
        }
    ]
}